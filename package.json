    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.51.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "dexie": "^4.0.8",
    "dexie-export-import": "^4.1.4",
    "joi": "^17.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
